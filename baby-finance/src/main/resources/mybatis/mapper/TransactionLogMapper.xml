<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dupake.finance.mapper.TransactionLogMapper">


    <resultMap id="authUserMap" type="com.dupake.finance.entity.TransactionLog">
        <id column="id" property="id"></id>
        <result property="business" column="business"/>
        <result property="foreignKey" column="foreign_key"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>

    </resultMap>

    <sql id="authUser_sql">
        id,
        business ,
        business ,
        create_time,
        update_time
    </sql>


    <!--新增-->
    <insert id="insert" parameterType="com.dupake.finance.entity.TransactionLog" useGeneratedKeys="true"
            keyProperty="id">
        insert into t_transaction_log(
        <trim suffixOverrides=",">
            <if test="id != null">
                id ,
            </if>
            <if test="createTime != null">
                create_time ,
            </if>
            <if test="updateTime != null">
                update_time ,
            </if>
            <if test="business != null and business != ''">
                business ,
            </if>
            <if test="foreignKey != null and foreignKey != ''">
                foreign_key ,
            </if>
        </trim>
        )
        values (
        <trim suffixOverrides=",">
            <if test="id != null">
                #{id},
            </if>
            <if test="createTime != null">
                #{createTime},
            </if>
            <if test="updateTime != null">
                #{updateTime},
            </if>
            <if test="business != null and business != ''">
                #{business},
            </if>
            <if test="foreignKey != null and foreignKey != ''">
                #{foreignKey},
            </if>
        </trim>

        )
    </insert>

    <select id="getByTransactionId" resultType="java.lang.Integer">
        select count(1) from t_transaction_log where id = #{transactionId}
    </select>


</mapper>